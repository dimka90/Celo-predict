{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://predinex.fly.dev/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://predinex.fly.dev",
    "NEXT_PUBLIC_API_BASE_URL": "https://predinex.fly.dev",
    "BACKEND_URL": "https://predinex.fly.dev",
    "NEXT_PUBLIC_BACKEND_URL": "https://predinex.fly.dev",
    "NEXT_PUBLIC_WS_URL": "wss://predinex.fly.dev",
    "NEXT_PUBLIC_RPC_URL": "https://alfajores-forno.celo.org",
    "NEXT_PUBLIC_CHAIN_ID": "44787"
  }
}
